{
  "parserOptions": {
    "ecmaVersion": 2017,
    "parserOptions.ecmaVersion": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "jsdoc",
    "jest"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended"
  ],
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true,
    "jasmine": true
  },
  "rules": {
    "indent": [
      "error",
      4
    ],
    "quotes": [
      "error",
      "double"
    ],
    "keyword-spacing": [
      "error",
      {
        "overrides": {
          "if": {
            "after": false
          },
          "for": {
            "after": false
          },
          "while": {
            "after": false
          },
          "switch": {
            "after": false
          }
        }
      }
    ],
    "switch-colon-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "one-var-declaration-per-line": "off",
    "one-var": "off",
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1
      }
    ],
    "dot-notation": "off",
    "no-console": "off",
    "max-len": [
      "error",
      240
    ],
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "no-bitwise": "warn",
    "camelcase": "off",
    "eqeqeq": "warn",
    "guard-for-in": "warn",
    "no-extend-native": "warn",
    "wrap-iife": "off",
    "no-use-before-define": "off",
    "new-cap": "off",
    "no-caller": "warn",
    "no-empty": "warn",
    "no-irregular-whitespace": "warn",
    "no-new": "off",
    "no-plusplus": "off",
    "no-undef": "warn",
    "no-unused-vars": "warn",
    "strict": "off",
    "max-params": "off",
    "max-depth": "off",
    "max-statements": "off",
    "complexity": "off",
    "no-var": "off",
    "semi": "off",
    "no-cond-assign": "off",
    "no-debugger": "off",
    "no-eq-null": "off",
    "no-eval": "off",
    "no-unused-expressions": "off",
    "block-scoped-var": "off",
    "padded-blocks": "off",
    "no-iterator": "off",
    "object-curly-spacing": ["error", "never"],
    "linebreak-style": "off",
    "comma-style": [
      "warn",
      "last"
    ],
    "comma-dangle": ["error", "never"],
    "brace-style": ["error", "1tbs", {"allowSingleLine": true}],
    "space-before-function-paren": ["error", "never"],
    "no-loop-func": "off",
    "no-multi-str": "off",
    "require-yield": "off",
    "valid-typeof": "off",
    "no-proto": "off",
    "no-script-url": "off",
    "no-shadow": "warn",
    "no-new-func": "off",
    "no-new-wrappers": "off",
    "no-invalid-this": "off",
    "jsdoc/checkAnnotations": "jsdoc3",
    "jsdoc/check-param-names": "warn",
    "jsdoc/check-tag-names": "warn",
    "jsdoc/check-types": "warn",
    "jsdoc/require-description-complete-sentence": "warn",
    "jsdoc/require-hyphen-before-param-description": "off",
    "jsdoc/require-param": "warn",
    "jsdoc/require-param-name": "warn",
    "jsdoc/require-param-type": "warn",
    "jsdoc/require-returns-type": "warn",
    "require-jsdoc": "off",
    "valid-jsdoc": ["warn", {
      "requireReturn": false,
      "requireReturnDescription": false,
      "requireParamDescription": false
    }],
    "jest/prefer-to-have-length": "off"
  }
}
